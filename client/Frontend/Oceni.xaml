<Page x:Class="Frontend.Oceni"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Frontend"
      mc:Ignorable="d" Background="#FFF1F3CE"
      d:DesignHeight="600" d:DesignWidth="900"
      Title="Oceni">

	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="17*"/>
			<RowDefinition Height="183*"/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="8*"/>
			<ColumnDefinition Width="17*"/>
		</Grid.ColumnDefinitions>
		<ListView HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="288" Grid.RowSpan="2" Background="#FFF1F3CE">
			<ListView.View>
				<GridView>
					<GridViewColumn/>
				</GridView>
			</ListView.View>
		</ListView>
		<StackPanel Grid.Column="1" Background="#FFED6A3D" HorizontalAlignment="Left" Width="612"/>
		<StackPanel Grid.Column="1" Background="#FF3E4959" Grid.Row="1" HorizontalAlignment="Left" Width="612">
			<StackPanel Height="70" Background="#FF4B86C6" Margin="25"/>
		</StackPanel>

	</Grid>
</Page>

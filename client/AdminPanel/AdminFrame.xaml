<Page x:Class="AdminPanel.AdminFrame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AdminPanel"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="600"
      Background="#3e5878"
      Title="AdminFrame">

    
        <UniformGrid Name ="Ugrid"  Columns="2" VerticalAlignment="Top">
        <Border Height="66" Background="{StaticResource MenuItemColor}" CornerRadius="10" Margin="10,10,-290,10" MouseLeftButtonDown="Transfer_Year">
            <Label Foreground="{StaticResource TextColor}" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Префрлување во нова учебна година"/>
        </Border>

    </UniformGrid>

</Page>
